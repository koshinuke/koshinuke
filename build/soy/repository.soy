{namespace org.koshinuke.template.repository}

/**
 * @param name
 */
{template .tmpl}
<div class="repository">
	<div class="repo-name">{$name}</div>
	<div class="repo-context">
		<div context="$$b" class="branches goog-tab">Branch</div>
		<div context="$$t" class="tags goog-tab">Tags</div>
		<div context="$$h" class="histories goog-tab">Histories</div>
		<div context="$$g" class="graph goog-tab">Graph</div>
	</div>
</div>
{/template}